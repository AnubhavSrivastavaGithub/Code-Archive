#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
typedef double db;

//Defines
#define pb push_back
#define f(n) for(ll i=0;i<n;i++)
#define fl(i,a,b) for(ll i=a;i<b;i++)
#define fastio ios_base::sync_with_stdio(false);cin.tie(NULL);

//Important Functions
ll gcd(ll a,ll b) { return b?gcd(b,a%b):a;}
ll lcm(ll a, ll b) { return a * (b / gcd(a, b)); }
#define min(a, b) (((a)<(b))?(a):(b))
#define max(a, b) (((a)<(b))?(b):(a))

//Templates
ll sqr(ll m){return m*m;}
//Main Function
int main() {
	// your code goes here
	ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int n,r,l,x,y;
    cin >> n >> l >> r>>x>>y;
    double distance = sqrt(double(sqr(x-l) + sqr(r-y)));
    if(distance==0)
    
    {
        cout << "0";
    }
    else
    {
        //cout << distance;
    int ans =0;
    ans += int(distance/(2*n));
    distance = distance - (int(distance/(2*n))*(2*n));
    //cout << distance;
    if(distance>0) ans+=1;
    cout << ans;
    }
    
	return 0;
}
